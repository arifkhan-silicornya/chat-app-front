{
  "name": "web",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "set PORT=3000 && cross-env NODE_OPTIONS='--inspect' next dev --turbopack",
    "linux_dev": "PORT=3000 cross-env NODE_OPTIONS='--inspect' next dev  --turbopack",
    "build": "next build && rm -rf .next/cache",
    "zip": "powershell -Command \"Compress-Archive -Path .next -DestinationPath .next.zip\"",
    "start": "PORT=3000 next start",
    "windows_start": "set PORT=3000 && next start",
    "lint": "eslint"
  },
  "dependencies": {
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4"
  }
}
